<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moonlit.generator.generator.mapper.GenDbMapper">

    <select id="selectAll" resultType="com.moonlit.generator.generator.entity.GenDb">
        SELECT * FROM `gen_db` WHERE 1 = 1
        <if test="null != genDb.dbName and '' != genDb.dbName">
            AND db_name LIKE CONCAT('%', #{genDb.dbName}, '%')
        </if>
        ORDER BY create_date DESC
    </select>

</mapper>