<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.moonlit.generator.generator.mapper.GenSystemConfigMapper">

    <select id="getSystemConfig" resultType="com.moonlit.generator.generator.entity.dto.SystemConfigDTO">
        SELECT salt, state
        FROM gen_system_config
        WHERE id = 1
    </select>
</mapper>